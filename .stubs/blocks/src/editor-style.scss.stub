// Editor-specific styles
.wp-block-{{namespace}}-{{blockName}} {
    // Editor-specific styles here
    padding: 1rem;
    margin: 1rem 0;
    background: #f5f5f5;
    border-radius: 4px;

    // Example of using SCSS features
    &__title {
        font-size: 1.5rem;
        margin-bottom: 1rem;
        color: #333;
    }

    &__content {
        line-height: 1.6;
    }
} 